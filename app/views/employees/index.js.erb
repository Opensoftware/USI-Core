<% if @employees.present? %>
  $("table.employees tbody").html('<%=raw escape_javascript(render :partial => "employee", :collection => @employees) %>');
<% else %>
  $("table.employees tbody").html('<%=raw content_tag(:tr, content_tag(:td, t(:label_employee_not_found), :colspan => 4, :style => "text-align: center;")) %>');
<% end %>
$("div.paginator").html('<%=raw will_paginate(@employees, :renderer => BootstrapPagination::Rails) %>');